{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "npm install --prefix frontend && npm install --prefix backend",
  "framework": null,
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/[[...path]]" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
